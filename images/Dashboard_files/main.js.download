// varailes

const hamburger = document.querySelector("#hamburger")
const column_one = document.querySelector("#column-one")
const closenav = document.querySelector("#close")



// functions

const open_nav = ()=>{
    column_one.classList.add("open")
}

const close_nav = ()=>{
    column_one.classList.remove("open")
}








// events
hamburger.addEventListener("click", open_nav)
closenav.addEventListener("click", close_nav)

